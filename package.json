{
	"name": "remix-auth-auth0",
	"version": "2.0.0",
	"keywords": [
		"remix",
		"remix-auth",
		"auth",
		"authentication",
		"strategy",
		"auth0"
	],
	"homepage": "https://github.com/danestves/remix-auth-auth0#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/danestves/remix-auth-auth0"
	},
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./build/index.js",
		"./package.json": "./package.json"
	},
	"files": [
		"build",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"exports": "bun run ./scripts/exports.ts",
		"quality": "biome check .",
		"quality:fix": "biome check . --write --unsafe",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@mjackson/headers": "0.10.0",
		"arctic": "3.5.0",
		"debug": "4.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@total-typescript/tsconfig": "1.0.4",
		"@types/bun": "1.2.5",
		"@types/debug": "4.1.12",
		"msw": "2.7.3",
		"typedoc": "0.28.1",
		"typedoc-plugin-mdn-links": "5.0.1",
		"typescript": "5.8.2"
	},
	"peerDependencies": {
		"remix-auth": "^4.0.0"
	},
	"engines": {
		"node": "^18.0.0 || ^20.0.0 || >=20.0.0"
	}
}
